{
  "name": "depslint",
  "version": "0.1.0",
  "description": "dependency linter",
  "keywords": [
    "dependency",
    "development dependency",
    "dependencies",
    "devDependencies",
    "lint",
    "check"
  ],
  "homepage": "https://github.com/commenthol/depslint",
  "bugs": {
    "url": "https://github.com/commenthol/depslint/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/depslint.git"
  },
  "license": "MIT",
  "author": "commenthol <commenthol@gmail.com>",
  "maintainers": [
    "commenthol <commenthol@gmail.com>"
  ],
  "type": "module",
  "main": "src/index.cjs",
  "types": "types",
  "bin": {
    "depslint": "./bin/depslint.js"
  },
  "files": [
    "bin",
    "src",
    "types"
  ],
  "scripts": {
    "ci": "npm-run-all clean lint c8 types",
    "clean": "rimraf coverage",
    "clean:all": "npm run clean && rimraf node_modules",
    "c8": "c8 -r text -r html -r lcov npm test",
    "lint": "eslint .",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "test": "mocha",
    "types": "rimraf types; tsc"
  },
  "mocha": {
    "checkLeaks": true,
    "colors": true,
    "exit": false
  },
  "dependencies": {
    "debug": "*",
    "depcheck": "^1.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "c8": "^10.1.2",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.8.0",
    "mocha": "^10.7.0",
    "npm-run-all2": "^6.2.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
